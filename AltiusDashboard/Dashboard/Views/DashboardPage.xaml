	x:Class="AltiusDashboard.DashboardPage">
	<ContentPage.BackgroundColor>
		<OnPlatform x:TypeArguments="Color" Android="{StaticResource darkBackgroundColor}">								
		</OnPlatform>	
	</ContentPage.BackgroundColor>
	<ContentPage.Content>
		<ScrollView>
